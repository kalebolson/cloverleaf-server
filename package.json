{
  "name": "cloverleafportal",
  "version": "1.0.0",
  "description": "Full stack solution for CL clients",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "proddebug": "set ENV=PROD&& node server.js",
    "server": "set ENV=DEV&& nodemon server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "start-all": "concurrently -k \"nodemon start\" \"npm run start-client\"",
    "start-client": "wait-on http://localhost:5000/ping/ && cd ./client && npm start"
  },
  "author": "Kaleb Olson",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.11.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.5.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "logrocket": "^2.2.0",
    "mongoose": "^6.1.1",
    "nodemailer": "^6.7.2",
    "pm2": "^5.2.0",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  }
}
